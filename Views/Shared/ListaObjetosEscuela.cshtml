@*
@model List<ObjetoEscuelaBase>
*@
<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nombre</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var objetoEscuela in Model)
    {
        <tr>
            <td>@objetoEscuela.UniqueId</td>
            <td>@objetoEscuela.Nombre</td>
        </tr>
        <!-- <partial name="ObjetoEscuelaSimple" model="alumno" />  -->
    }   
    </tbody>
</table>
