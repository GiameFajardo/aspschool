@model List<Alumno>

@{
    ViewData["Title"] = "Alumnos";
    Layout = "Simple";
}
<h1>@ViewData["Title"] </h1>




<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nombre</th>
        </tr>
    </thead>
    <tbody>
    @foreach (Alumno alumno in Model)
    {
        <tr>
            <td>@alumno.UniqueId</td>
            <td>@alumno.Nombre</td>
        </tr>
        <!-- <partial name="ObjetoEscuelaSimple" model="alumno" />  -->
    }   
    </tbody>
</table>

<p><i>@ViewBag.CosaDinamica -  @ViewData["Fecha"]</i></p>